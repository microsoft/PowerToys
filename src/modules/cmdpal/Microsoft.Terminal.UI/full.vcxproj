        <EnableVectorLength Condition="'%(ClCompile.EnableEnhancedInstructionSet)' != 'AdvancedVectorExtensions512' and '%(ClCompile.EnableEnhancedInstructionSet)' != 'AdvancedVectorExtensions101'" />
      </ClCompile>
    </ItemGroup>
