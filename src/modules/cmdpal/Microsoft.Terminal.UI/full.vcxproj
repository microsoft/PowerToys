  <Target Name="ComputeStdModulesCompileInputs" Condition="'@(ClCompile)' != '' and '$(EnableCppModules)' != 'false' and '$(EnableStdModules)' != 'false'" DependsOnTargets="SetCppLanguageStandardCapabilities">
